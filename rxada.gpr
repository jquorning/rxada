with "rxada_common";

project RxAda is

   for Source_Dirs use ("src/**");
   for Excluded_Source_Dirs use ("src/bugs",
                                 "src/main");

   for Object_Dir use "obj";

   package Ide renames RxAda_Common.Ide;

   package Builder renames RxAda_Common.Builder;

   package Compiler renames RxAda_Common.Compiler;

   package Binder renames RxAda_Common.Binder;

   package Gnattest renames RxAda_Common.Gnattest;

end RxAda;
